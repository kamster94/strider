<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane prefHeight="415.0" prefWidth="545.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="desktopGui.ControllerRateHotel">
   <children>
      <VBox>
         <children>
            <HBox prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
               <children>
                  <VBox HBox.hgrow="ALWAYS">
                     <children>
                        <VBox alignment="CENTER" spacing="10.0">
                           <children>
                              <Label text="Wybierz odwiedzony hotel" />
                              <ComboBox fx:id="combobox_hotel" prefWidth="150.0" />
                           </children>
                        </VBox>
                        <HBox prefHeight="100.0" prefWidth="200.0">
                           <children>
                              <VBox fx:id="vbox_rateclean" alignment="CENTER" spacing="10.0" HBox.hgrow="ALWAYS">
                                 <children>
                                    <Label text="Czystość" />
                                 </children>
                                 <padding>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </padding>
                              </VBox>
                              <VBox fx:id="vbox_ratecomfort" alignment="CENTER" spacing="10.0" HBox.hgrow="ALWAYS">
                                 <children>
                                    <Label text="Komfort" />
                                 </children>
                                 <padding>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </padding>
                              </VBox>
                           </children>
                        </HBox>
                        <HBox prefHeight="100.0" prefWidth="200.0">
                           <children>
                              <VBox fx:id="vbox_ratelocalization" alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="10.0" HBox.hgrow="ALWAYS">
                                 <children>
                                    <Label text="Lokalizacja" />
                                 </children>
                                 <padding>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </padding>
                              </VBox>
                              <VBox fx:id="vbox_rateudogothings" alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="10.0" HBox.hgrow="ALWAYS">
                                 <children>
                                    <Label text="Udogodnienia" />
                                 </children>
                                 <padding>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </padding>
                              </VBox>
                           </children>
                        </HBox>
                        <HBox prefHeight="100.0" prefWidth="200.0">
                           <children>
                              <VBox fx:id="vbox_ratepersonel" alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="10.0" HBox.hgrow="ALWAYS">
                                 <children>
                                    <Label text="Personel" />
                                 </children>
                                 <padding>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </padding>
                              </VBox>
                              <VBox fx:id="vbox_ratequalitytoprice" alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="10.0" HBox.hgrow="ALWAYS">
                                 <children>
                                    <Label text="Stosunek jakości do ceny" />
                                 </children>
                              </VBox>
                           </children>
                        </HBox>
                     </children>
                  </VBox>
                  <VBox spacing="10.0" HBox.hgrow="ALWAYS">
                     <children>
                        <TitledPane collapsible="false" prefHeight="200.0" prefWidth="200.0" text="Dane hotelu">
                           <content>
                              <VBox prefHeight="200.0" prefWidth="100.0">
                                 <children>
                                    <Label fx:id="label_hotelname" text="Label" />
                                    <Label fx:id="label_streetnameandnumber" text="Label" />
                                    <Label fx:id="label_zipcitycountry" text="Label" />
                                 </children>
                              </VBox>
                           </content>
                        </TitledPane>
                        <TitledPane collapsible="false" prefHeight="200.0" prefWidth="200.0" text="Notatki">
                           <content>
                              <TextArea fx:id="textarea_notes" prefHeight="200.0" prefWidth="200.0" />
                           </content>
                        </TitledPane>
                        <VBox fx:id="vbox_rateaverage" alignment="CENTER" spacing="10.0">
                           <children>
                              <Label text="Średnia ocen" />
                           </children>
                           <padding>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </padding>
                        </VBox>
                     </children>
                  </VBox>
               </children>
            </HBox>
            <HBox alignment="CENTER" VBox.vgrow="NEVER">
               <children>
                  <Button fx:id="button_cancel" mnemonicParsing="false" text="Anuluj" />
                  <Separator visible="false" HBox.hgrow="ALWAYS" />
                  <Button fx:id="button_rate" mnemonicParsing="false" text="Oceń" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</StackPane>
