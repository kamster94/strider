<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>


<StackPane prefHeight="388.0" prefWidth="411.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="desktopGui.ControllerChangeUserOptions">
   <children>
      <VBox>
         <children>
            <VBox VBox.vgrow="ALWAYS">
               <children>
                  <HBox prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                     <children>
                        <VBox fx:id="vbox_countrybox" spacing="10.0" HBox.hgrow="ALWAYS">
                           <children>
                              <Label text="Państwo" />
                           </children>
                        </VBox>
                        <VBox fx:id="vbox_citybox" spacing="10.0" HBox.hgrow="ALWAYS">
                           <children>
                              <Label text="Miasto" />
                           </children>
                        </VBox>
                     </children>
                  </HBox>
                  <HBox prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                     <children>
                        <VBox fx:id="vbox_currency" spacing="10.0" HBox.hgrow="ALWAYS">
                           <children>
                              <Label text="Waluta" />
                           </children>
                        </VBox>
                     </children>
                  </HBox>
               </children>
            </VBox>
            <HBox spacing="10.0">
               <children>
                  <Button fx:id="button_cancel" mnemonicParsing="false" text="Anuluj" />
                  <Separator visible="false" HBox.hgrow="ALWAYS" />
                  <Button fx:id="button_apply" mnemonicParsing="false" text="Zatwierdź" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</StackPane>
