<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="600.0" minWidth="800.0" prefHeight="600.0" prefWidth="800.0" stylesheets="@ta_mainwindow.css" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <HBox alignment="CENTER" spacing="10.0" styleClass="upperhbox" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="labelwelcomemessage" alignment="CENTER" contentDisplay="CENTER" text="TODO : WELCOME MESSAGE">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" />
         </padding>
      </HBox>
   </top>
   <bottom>
      <HBox alignment="CENTER" spacing="10.0" styleClass="lowerhbox" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="labelaccountstatus" text="TODO : ACCOUNT STATUS MESSAGE">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Separator prefHeight="0.0" prefWidth="149.0" visible="false" HBox.hgrow="ALWAYS" />
            <Label fx:id="labelversion" text="TODO : VERSION">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
         </children>
         <padding>
            <Insets left="5.0" right="5.0" top="5.0" />
         </padding>
      </HBox>
   </bottom>
   <center>
      <SplitPane dividerPositions="0.5" prefHeight="160.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <items>
            <VBox alignment="CENTER" spacing="5.0" styleClass="leftvbox">
               <children>
                  <ScrollPane fitToWidth="true" prefHeight="166.0" prefWidth="386.0" styleClass="Pane" vbarPolicy="ALWAYS" VBox.vgrow="ALWAYS">
                     <content>
                        <VBox alignment="CENTER" spacing="10.0" styleClass="Pane">
                           <padding>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </padding>
                           <children>
                              <TitledPane fx:id="titledpanetravel" animated="false" text="Travel">
                                 <content>
                                    <VBox alignment="CENTER" spacing="10.0" styleClass="vbox">
                                       <children>
                                          <ToggleButton fx:id="togglebuttoncreatenewtravel" minHeight="50.0" minWidth="250.0" mnemonicParsing="false" styleClass="button" text="Create new travel">
                                             <font>
                                                <Font size="14.0" />
                                             </font>
                                          </ToggleButton>
                                          <ToggleButton fx:id="togglebuttonviewcurrenttravel" minHeight="50.0" minWidth="250.0" mnemonicParsing="false" styleClass="button" text="View current travel">
                                             <font>
                                                <Font size="14.0" />
                                             </font>
                                          </ToggleButton>
                                          <ToggleButton fx:id="togglebuttonadditionalinformations" minHeight="50.0" minWidth="250.0" mnemonicParsing="false" styleClass="button" text="Additional informations">
                                             <font>
                                                <Font size="14.0" />
                                             </font>
                                          </ToggleButton>
                                          <ToggleButton fx:id="togglebuttontravelhistory" minHeight="50.0" minWidth="250.0" mnemonicParsing="false" styleClass="button" text="Travel history">
                                             <font>
                                                <Font size="14.0" />
                                             </font>
                                          </ToggleButton>
                                       </children>
                                       <padding>
                                          <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                       </padding>
                                    </VBox>
                                 </content>
                                 <font>
                                    <Font size="18.0" />
                                 </font>
                              </TitledPane>
                              <TitledPane fx:id="titledpaneaccomodation" animated="false" text="Accomodation">
                                 <font>
                                    <Font size="18.0" />
                                 </font>
                                 <VBox.margin>
                                    <Insets />
                                 </VBox.margin>
                                 <content>
                                    <VBox alignment="CENTER" spacing="10.0" styleClass="vbox">
                                       <padding>
                                          <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                       </padding>
                                       <children>
                                          <ToggleButton fx:id="togglebuttonreviewhotels" minHeight="50.0" minWidth="250.0" mnemonicParsing="false" styleClass="button" text="Review hotels">
                                             <font>
                                                <Font size="14.0" />
                                             </font>
                                          </ToggleButton>
                                       </children>
                                    </VBox>
                                 </content>
                              </TitledPane>
                              <TitledPane fx:id="titledpanesettings" animated="false" text="Settings">
                                 <font>
                                    <Font size="18.0" />
                                 </font>
                                 <content>
                                    <VBox alignment="CENTER" spacing="10.0">
                                       <padding>
                                          <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                       </padding>
                                    </VBox>
                                 </content>
                              </TitledPane>
                           </children>
                        </VBox>
                     </content>
                  </ScrollPane>
                  <Button fx:id="buttonexit" minHeight="50.0" minWidth="250.0" mnemonicParsing="false" stylesheets="@ta_mainwindow.css" text="Exit">
                     <font>
                        <Font size="14.0" />
                     </font></Button>
               </children>
               <padding>
                  <Insets bottom="5.0" />
               </padding>
            </VBox>
            <ScrollPane fitToHeight="true" fitToWidth="true">
               <content>
                  <StackPane prefHeight="150.0" prefWidth="200.0">
                     <children>
                        <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0" visible="false">
                           <children>
                              <Label text="DANK MEME 1">
                                 <font>
                                    <Font size="36.0" />
                                 </font>
                              </Label>
                           </children>
                           <padding>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </padding>
                        </VBox>
                        <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0" visible="false">
                           <children>
                              <Label text="Current travel">
                                 <font>
                                    <Font size="24.0" />
                                 </font>
                              </Label>
                           </children>
                           <padding>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </padding>
                        </VBox>
                     </children>
                  </StackPane>
               </content>
            </ScrollPane>
         </items>
      </SplitPane>
   </center>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</BorderPane>
