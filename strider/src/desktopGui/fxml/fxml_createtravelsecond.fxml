                                    <TitledPane expanded="false" text="Adres">
                                       <content>
                                          <HBox spacing="10.0">
                                             <children>
                                                <VBox HBox.hgrow="ALWAYS">
                                                   <children>
                                                      <VBox>
                                                         <children>
                                                            <Label text="Państwo" />
                                                            <HBox fx:id="h_hbox_countrybox" />
                                                         </children>
                                                      </VBox>
                                                      <Label text="Nazwa" />
                                                      <TextField fx:id="h_textfield_name" />
