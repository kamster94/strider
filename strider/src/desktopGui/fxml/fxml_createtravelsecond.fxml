                                                   <children>
                                                      <VBox>
                                                         <children>
                                                            <Label text="Pańśtwo" />
                                                            <HBox fx:id="t_hbox_countrybox_end" />
                                                         </children>
                                                      </VBox>
                                                      <Label text="Data" />
                                                      <DatePicker fx:id="t_datepicker_end" />
                                                   </children>
                                                </VBox>
                                                <VBox HBox.hgrow="ALWAYS">
                                                   <children>
                                                      <VBox>
                                                         <children>
                                                            <Label text="Miasto" />
                                                            <HBox fx:id="t_hbox_citybox_end" />
                                                         </children>
                                                      </VBox>
                                                      <Label text="Godzina" />
                                                      <TextField fx:id="t_textfield_hour_end" />
