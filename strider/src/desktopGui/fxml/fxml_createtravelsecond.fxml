                                                                  <TextField fx:id="t_textfield_price" maxWidth="100.0" />
                                                               </children>
                                                            </VBox>
                                                            <VBox HBox.hgrow="ALWAYS">
                                                               <children>
                                                                  <Label text="Waluta" />
                                                                  <HBox fx:id="t_hbox_currencybox" />
