               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
            <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
              <tabs>
                <Tab text="Atrakcje">
                     <content>
                        <HBox>
                           <children>
                              <Accordion HBox.hgrow="ALWAYS">
                                 <panes>
                                    <TitledPane expanded="false" text="Adres">
                                       <content>
                                          <HBox spacing="10.0">
                                             <children>
                                                <VBox spacing="10.0" HBox.hgrow="ALWAYS">
                                                   <children>
                                                      <VBox>
                                                         <children>
                                                            <Label text="Państwo" />
                                                            <HBox fx:id="a_hbox_countrybox" spacing="10.0" />
                                                         </children>
                                                      </VBox>
                                                      <Label text="Nazwa" />
                                                      <TextField fx:id="a_textfield_name" />
                                                   </children>
                                                </VBox>
                                                <VBox spacing="10.0" HBox.hgrow="ALWAYS">
                                                   <children>
                                                      <VBox>
                                                         <children>
                                                            <Label text="Miasto" />
                                                            <HBox fx:id="a_hbox_citybox" spacing="10.0" />
                                                         </children>
                                                      </VBox>
                                                      <Label text="Ulica" />
                                                      <TextField fx:id="a_textfield_street" />
                                                   </children>
                                                </VBox>
                                                <VBox spacing="10.0" HBox.hgrow="ALWAYS">
                                                   <children>
                                                      <Label text="Kod pocztowy" />
                                                      <TextField fx:id="a_textfield_zipcode" maxWidth="100.0" />
                                                      <Label text="Nr" />
                                                      <TextField fx:id="a_textfield_number" maxWidth="50.0" />
                                                   </children>
                                                </VBox>
                                             </children>
                                             <padding>
                                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                             </padding>
                                          </HBox>
                                       </content>
                                    </TitledPane>
                                    <TitledPane expanded="false" text="Godziny otwarcia">
                                       <content>
                                          <HBox spacing="10.0">
                                             <children>
                                                <VBox spacing="10.0" HBox.hgrow="ALWAYS">
                                                   <children>
                                                      <Label text="Od" />
                                                      <TextField fx:id="a_textfield_openfrom" maxWidth="100.0" />
                                                   </children>
                                                </VBox>
                                                <VBox spacing="10.0" HBox.hgrow="ALWAYS">
                                                   <children>
                                                      <Label text="Do" />
                                                      <TextField fx:id="a_textfield_opentill" maxWidth="100.0" />
                                                   </children>
                                                </VBox>
                                             </children>
                                             <padding>
                                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                             </padding>
                                          </HBox>
                                       </content>
                                    </TitledPane>
                                    <TitledPane expanded="false" text="Opłaty">
                                       <content>
                                          <HBox spacing="10.0">
                                             <children>
                                                <VBox HBox.hgrow="ALWAYS">
                                                   <HBox.margin>
                                                      <Insets />
                                                   </HBox.margin>
                                                   <children>
                                                      <Label text="Cena" />
                                                      <TextField fx:id="a_textfield_price" maxWidth="100.0" />
                                                   </children>
                                                </VBox>
                                                <VBox HBox.hgrow="ALWAYS">
                                                   <children>
                                                      <Label text="Waluta" />
                                                      <HBox fx:id="a_hbox_currencybox" spacing="10.0" />
                                                   </children>
                                                </VBox>
                                             </children>
                                             <padding>
                                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                             </padding>
                                          </HBox>
                                       </content>
                                    </TitledPane>
                                    <TitledPane text="Moje uwagi">
                                       <content>
                                          <TextArea fx:id="a_textarea_notes" prefHeight="200.0" prefWidth="200.0" />
                                       </content>
                                    </TitledPane>
                                 </panes>
                                 <padding>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </padding>
                              </Accordion>
                              <VBox spacing="10.0" HBox.hgrow="ALWAYS">
                                 <children>
                                    <Label text="Warto zobaczyć" />
