                                                         </children>
                                                      </VBox>
                                                      <Label text="Ulica" />
                                                      <TextField fx:id="h_textfield_street" />
                                                   </children>
                                                </VBox>
                                                <VBox HBox.hgrow="ALWAYS">
                                                   <children>
                                                      <Label text="Kod pocztowy" />
                                                      <TextField fx:id="h_textfield_zipcode" maxWidth="100.0" />
                                                      <Label text="Nr" />
                                                      <TextField fx:id="h_textfield_number" maxWidth="50.0" />
                                                   </children>
                                                </VBox>
                                             </children>
                                             <padding>
                                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                             </padding>
                                          </HBox>
                                       </content>
                                    </TitledPane>
                                    <TitledPane expanded="false" text="Pobyt">
                                       <content>
                                          <HBox spacing="10.0">
                                             <children>
                                                <VBox HBox.hgrow="ALWAYS">
                                                   <children>
                                                      <Label text="Od" />
                                                      <HBox spacing="10.0">
                                                         <children>
                                                            <VBox HBox.hgrow="ALWAYS">
                                                               <children>
                                                                  <Label text="Data" />
                                                                  <DatePicker fx:id="h_datepicker_start" disable="true" />
                                                               </children>
                                                            </VBox>
                                                            <VBox HBox.hgrow="ALWAYS">
                                                               <children>
                                                                  <Label text="Godzina" />
                                                                  <TextField fx:id="h_textfield_hour_start" />
                                                               </children>
                                                            </VBox>
                                                         </children>
                                                      </HBox>
                                                      <Label text="Do" />
                                                      <HBox spacing="10.0">
                                                         <children>
                                                            <VBox HBox.hgrow="ALWAYS">
                                                               <children>
                                                                  <Label text="Data" />
                                                                  <DatePicker fx:id="h_datepicker_end" />
                                                               </children>
                                                            </VBox>
                                                            <VBox HBox.hgrow="ALWAYS">
                                                               <children>
                                                                  <Label text="Godzina" />
                                                                  <TextField fx:id="h_textfield_hour_end" />
                                                               </children>
                                                            </VBox>
                                                         </children>
                                                      </HBox>
                                                   </children>
                                                </VBox>
                                                <VBox HBox.hgrow="ALWAYS" />
